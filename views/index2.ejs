<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="styles/main.css">
</head>

<body>
    <div class="flex-container">
    <form action="/signup" method="post">
      <h1 class="h3 mb-3 fw-normal d-flex justify-content-center htext">SIGN UP</h1>
    
        <div class="form-floating mt-3">
          <input name="email" type="email" class="form-control" id="floatingInput" placeholder="name@example.com" fdprocessedid="nu3jz" required="">
          <label for="floatingInput">Email address</label>
        </div>

        <div class="form-floating mt-3">
          <input name="password" type="password" class="form-control" id="floatingPassword" placeholder="Password" fdprocessedid="8x7ek" required="">
          <label for="floatingPassword">Password</label>
        </div>
    
        <div class="form-check text-start my-3">
          <input class="form-check-input" type="checkbox" value="remember-me" id="flexCheckDefault">
          <label class="form-check-label" for="flexCheckDefault">
            Remember me
          </label>
      

        </div>
        <button class="btn btn-primary w-100 py-2 mb-3" type="submit" fdprocessedid="z8dep" style="background-color: #2a6049 !important; color: #d8dacd !important;">Sign up</button>
        <p>Already a member? <a href="/">login</a></p>
        <% const d = new Date(); %> 
        <% let year = d.getFullYear(); %> 
        <p class="mt-2 mb-3 text-body-secondary">Â© <%- year %></p>
      </form>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
    </div>
</body>
</html>